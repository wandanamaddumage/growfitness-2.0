# Base URL for API requests (include /api suffix)
# If admin and API are on the same host and Nginx proxies /api -> api:3000, leave as /api
VITE_API_BASE_URL=https://api.your-domain.com/api

# Google Maps JavaScript API key (for location map picker)
# Enable "Maps JavaScript API" in Google Cloud Console
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
